tryalias ma makeanywhere
tryalias pma 'pipenv run makeanywhere'

tryalias pass $HOME/bin/pass-with-custom-editor.sh

tryalias em 'emacs -nw'
tryalias emc 'emacsclient -nw --alternate-editor=""'
tryalias emacsc 'emacsclient --alternate-editor=""'
tryalias mv 'mv -i'
tryalias cp 'cp -i'
tryalias rm 'rm -i'

tryalias ls exa ls
tryalias l 'ls -Fa'
tryalias ll 'l -lh'
tryalias gzip pigz gzip

tryalias c ccat
tryalias exa 'exa --git' exa

tryalias trizen 'trizen --flip-results=1'

tryalias py python3 python
tryalias ipy ipython3 ipython

tryalias v nvim vim vis vi nano ed libreoffice openoffice 'wine MicrosoftOffice2003.exe' vim
tryalias vmin 'nvim -u ~/.vimrc.minimal' 'vim -u ~/.vimrc.minmal' vis vi nano 'nvim -u ~/.vimrc.minimal'
tryalias e 'zathura --fork' evince mupdf mupdf-x11 qpdfview 'wine READER10.exe'

tryalias pmake 'pipenv run make'

tryalias kubectl-get-pods-with-node 'kubectl get pod -o wide'
tryalias kubectl-get-previous-logs 'kubectl logs --previous'

